import random

# Add an element to fruits with the key 'grape' and the value 5
def to_letter(n):
  if n == 0:
    return 'zero'
  elif n == 1:
    return 'one'
  elif n == 2:
    return 'two'
  elif n == 3:
    return 'three'
  elif n == 4:
    return 'four'
  elif n == 5:
    return 'five'
  elif n == 6:
    return 'six'
  elif n == 7:
    return 'seven'
  elif n == 8:
    return 'eight'
  elif n == 9:
    return 'nine'
  else:
    print('Ten')

# Print fruits
nums = {}
for num in range(0,10):
  nums[to_letter(num)] = num
  
print(nums)
  
  
